{
    "name": "esscalc",
    "publisher": "vscode-essbase",
    "displayName": "EssCalc",
    "description": "Syntax Highlighting for Essbase Calculations",
    "version": "0.0.2",
    "repository": "https://github.com/phoch00/VSC-Language-EssCalc-2022",
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "csc",
            "aliases": ["EssCalc", "csc"],
            "extensions": [".csc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "csc",
            "scopeName": "source.esscalc",
            "path": "./syntaxes/csc.tmLanguage.json"
        }],
        "snippets": [{
            "language": "csc",
            "path": "./snippets/csc.json"
        }],
        "configuration": {
            "type": "object",
            "title": "Essbase Calc Script Language",
            "properties": {
                "csc.maxNumberOfProblems": {
                    "type": "number",
                    "default": 100,
                    "description": "Controls the maximum number of problems produced by the server."
                }
            }
        },
        "configurationDefaults": {
            "[csc]": {
                "editor.tabSize": 4,
                "editor.insertSpaces": false,
                "editor.detectIndentation": true,
                "editor.trimAutoWhitespace": true,
                "editor.autoClosingBrackets": "always"
            }
        }
    }

}